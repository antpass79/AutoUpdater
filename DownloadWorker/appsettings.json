{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Download": {
    "Url": "https://<<storageaccount>>.blob.core.windows.net/container/blob", //https://localhost:5001/Download,
    "OutputFolder": "Temp\\",
    "ParallelDownloads": 10,
    "AZCopy": {
      "Token": "<<SASToken>>",
      "FileName": "localfilename"
    }
  }
}
